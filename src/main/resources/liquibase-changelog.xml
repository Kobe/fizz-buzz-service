<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
   http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">

    <changeSet author="Kobe" id="1">
        <createTable tableName="fizz_buzz">
            <column autoIncrement="true" name="id" type="INT">
                <constraints primaryKey="true"/>
            </column>
            <column name="input" type="INT">
                <constraints nullable="false"/>
            </column>
            <column name="output" type="VARCHAR(255)" defaultValue="">
                <constraints nullable="true"/>
            </column>
            <column name="time" type="VARCHAR(255)" defaultValue="unknown">
                <constraints nullable="true"/>
            </column>
            <column name="timestamp" type="INT" defaultValue="0">
                <constraints nullable="true"/>
            </column>
        </createTable>
        <rollback>
            <dropTable tableName="fizz_buzz"/>
        </rollback>
    </changeSet>

</databaseChangeLog>